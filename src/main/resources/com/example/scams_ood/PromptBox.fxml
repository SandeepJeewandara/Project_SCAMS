<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="promptBoxPain" prefHeight="150.0" prefWidth="398.0" style="-fx-border-color: red; -fx-border-width: 2px;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Features.PromptController">
   <children>
      <Label fx:id="promptText" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="31.0" prefHeight="22.0" prefWidth="400.0" text="Successfully Signed Up!" textAlignment="CENTER">
         <font>
            <Font name="Segoe UI Semibold" size="15.0" />
         </font>
      </Label>
      <Button fx:id="okButton" layoutX="168.0" layoutY="100.0" mnemonicParsing="false" onAction="#onOkButtonClick" prefHeight="28.0" prefWidth="61.0" style="-fx-background-radius: 40; -fx-background-color: #813EB6;" text="OK" textFill="#fafafa">
         <font>
            <Font name="Tahoma Bold" size="13.5" />
         </font>
      </Button>
   </children>
</AnchorPane>
